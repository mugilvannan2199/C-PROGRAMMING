#include<stdio.h>

main()
{
   int a,b,c;
   printf("enter number for a: ");
   scanf("%d",&a);
   printf("enter number for b: ");
   scanf("%d",&b);
   printf("enter number for c: ");
   scanf("%d",&c);

   if(a>b && a>c){
       printf("a is the biggest");
       }
   else if(b>a && b>c){
       printf("b is the biggest number");
       }
   else{
       printf("c is the biggest number");}
}
